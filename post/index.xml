&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">&lt;span class="n">Synced&lt;/span> &lt;span class="n">times&lt;/span> &lt;span class="n">from&lt;/span> &lt;span class="n">source&lt;/span> &lt;span class="s1">&amp;#39;test&amp;#39;&lt;/span> &lt;span class="n">to&lt;/span> &lt;span class="n">target&lt;/span> &lt;span class="s1">&amp;#39;test2&amp;#39;&lt;/span>&lt;span class="p">.&lt;/span>
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">&lt;span class="nb">PS &lt;/span>&lt;span class="n">D:&lt;/span>&lt;span class="p">\&lt;/span>&lt;span class="n">bin&lt;/span>&lt;span class="p">\&lt;/span>&lt;span class="n">Debug&lt;/span>&lt;span class="p">&amp;gt;&lt;/span> &lt;span class="p">.\&lt;/span>&lt;span class="n">SharpFileTimeSetter&lt;/span>&lt;span class="p">.&lt;/span>&lt;span class="py">exe&lt;/span> &lt;span class="p">-&lt;/span>&lt;span class="o">-file&lt;/span> &lt;span class="n">test2&lt;/span> &lt;span class="p">-&lt;/span>&lt;span class="n">-get&lt;/span>
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">&lt;span class="n">File&lt;/span>&lt;span class="err">:&lt;/span> &lt;span class="n">test2&lt;/span>
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">&lt;span class="n">Creation&lt;/span> &lt;span class="n">Time&lt;/span>&lt;span class="err">:&lt;/span> &lt;span class="mf">1999&lt;/span>&lt;span class="p">/&lt;/span>&lt;span class="mf">9&lt;/span>&lt;span class="p">/&lt;/span>&lt;span class="mf">9&lt;/span> &lt;span class="mf">12&lt;/span>&lt;span class="err">:&lt;/span>&lt;span class="mf">00&lt;/span>&lt;span class="err">:&lt;/span>&lt;span class="mf">00&lt;/span>
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">&lt;span class="n">Modification&lt;/span> &lt;span class="n">Time&lt;/span>&lt;span class="err">:&lt;/span> &lt;span class="mf">1999&lt;/span>&lt;span class="p">/&lt;/span>&lt;span class="mf">9&lt;/span>&lt;span class="p">/&lt;/span>&lt;span class="mf">9&lt;/span> &lt;span class="mf">12&lt;/span>&lt;span class="err">:&lt;/span>&lt;span class="mf">00&lt;/span>&lt;span class="err">:&lt;/span>&lt;span class="mf">00&lt;/span>
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">&lt;span class="n">Access&lt;/span> &lt;span class="n">Time&lt;/span>&lt;span class="err">:&lt;/span> &lt;span class="mf">2025&lt;/span>&lt;span class="p">/&lt;/span>&lt;span class="mf">1&lt;/span>&lt;span class="p">/&lt;/span>&lt;span class="mf">25&lt;/span> &lt;span class="mf">13&lt;/span>&lt;span class="err">:&lt;/span>&lt;span class="mf">19&lt;/span>&lt;span class="err">:&lt;/span>&lt;span class="mf">54&lt;/span>
