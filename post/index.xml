&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl"> &lt;span class="p">-&lt;/span>&lt;span class="n">-modification&lt;/span> &lt;span class="n">Modification&lt;/span> &lt;span class="n">time&lt;/span> &lt;span class="n">to&lt;/span> &lt;span class="nb">set &lt;/span>&lt;span class="p">(&lt;/span>&lt;span class="n">optional&lt;/span>&lt;span class="p">,&lt;/span> &lt;span class="n">format&lt;/span>&lt;span class="err">:&lt;/span> &lt;span class="nb">yyyy-MM&lt;/span>&lt;span class="n">-dd&lt;/span> &lt;span class="n">HH&lt;/span>&lt;span class="err">:&lt;/span>&lt;span class="n">mm&lt;/span>&lt;span class="err">:&lt;/span>&lt;span class="n">ss&lt;/span>&lt;span class="p">).&lt;/span>
